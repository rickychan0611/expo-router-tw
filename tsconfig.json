{
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@stores/*": ["src/stores/*"],
      "@utils/*": ["src/utils/*"],
      "@assets/*": ["assets/*"],
      "@api/*": ["src/api/*"],
      "@models/*": ["src/models/*"],
      "@hooks/*": ["src/hooks/*"],
    }
  },
  "extends": "expo/tsconfig.base",
}